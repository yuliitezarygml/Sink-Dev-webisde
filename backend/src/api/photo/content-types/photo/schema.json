{
  "kind": "collectionType",
  "collectionName": "photos",
  "info": {
    "singularName": "photo",
    "pluralName": "photos",
    "displayName": "Photo"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": false
    }
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "order": {
      "type": "integer"
    },
    "tags": {
      "type": "string"
    },
    "gallery": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::gallery.gallery",
      "inversedBy": "photos"
    }
  }
}
